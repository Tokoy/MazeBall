{
  "name": "MazeBall",
  "description": "Left-click to control the ball's horizontal movement, right-click to move it downward.",
  "version": "1.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "host_permissions": ["https://game.ikeno.top/"],
  "action": {
      "default_popup": "popup.html",
      "default_title": "MazeBall"
  },
  "icons": {
    "128": "img/mazeball.png"
  }
}