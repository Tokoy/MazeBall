{
  "name": "MazeBall",
  "description": "鼠标左键控制小球左右移动，鼠标右键控制小球向下移动。",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["storage","https://game.ikeno.top/*"],
  "browser_action": {
      "default_popup": "popup.html",
      "default_title": "MazeBall"
  },
  "icons": {
    "128": "img/mazeball.png"
  }
}